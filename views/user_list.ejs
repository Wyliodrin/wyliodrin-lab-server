<% include _header %>
    <h1>Wyliodrin Pi server</h1>
    <% users.forEach(function(user) { %>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="/users/<%= user.username%>">
                    <%= user.fullName() %>
                </a>
            </div>
            <% if (user.bio) { %>
                <div class="panel-body">
                    <%= user.bio %>
                </div>
                <% } %>
        </div>
        <% }) %>
            <% include _footer %>